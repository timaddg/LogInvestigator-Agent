# Log Investigator Environment Configuration
# Copy this file to .env and update with your actual values

# Required: Google Gemini API Key
# Get your API key from: https://makersuite.google.com/app/apikey
GEMINI_API_KEY=your_gemini_api_key_here

# Optional: Gemini Configuration (Updated to 2.5 Pro)
GEMINI_MODEL=gemini-2.5-pro
GEMINI_MAX_TOKENS=2048
GEMINI_TEMPERATURE=0.3

# Optional: Log Processing Configuration
ENABLE_LOG_OPTIMIZATION=true
MAX_INPUT_TOKENS=30000
MAX_LOG_ENTRIES=1000
SAMPLE_SIZE=500

# Optional: Application Configuration
LOG_FILE=log_investigator.log
LOG_LEVEL=INFO
SECRET_KEY=your-secret-key-change-this

# Optional: File Paths
SAMPLE_LOGS_FILE=data/logs/sample_logs.json 